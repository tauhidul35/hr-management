- if employee_signed_in?
  = link_to 'Logout', destroy_employee_session_path, method: :delete
- elsif admin_signed_in?
  = link_to 'Add Attendances', form_admins_attendances_path
  = link_to 'Employees', admins_employees_path
  = link_to 'Logout', destroy_admin_session_path, method: :delete
- else
  = link_to 'Employee Login', employee_google_oauth2_omniauth_authorize_path, method: :post
  = link_to 'Admin Login', new_admin_session_path
